{
    "routes": [
        {
            "route": "/api/*", 
            "methods": ["GET", "POST"]
        }
    ],
    "platform": {
          "apiRuntime": "python:3.10"
        },
    "navigationFallback": {
        "rewrite": "/index.html"
      },
        "auth": {
          "identityProviders": {
            "customOpenIdConnectProviders": {
              "aadb2c": {
                "registration": {
                  "clientIdSettingName": "AADB2C_PROVIDER_CLIENT_ID",
                  "clientCredential": {
                    "clientSecretSettingName": "AADB2C_PROVIDER_CLIENT_SECRET"
                  },
                  "openIdConnectConfiguration": {
                    "wellKnownOpenIdConfiguration": "https://storyfairy.b2clogin.com/storyfairy.onmicrosoft.com/B2C_1_Storyfairy_SUSI/v2.0/.well-known/openid-configuration"
                  }
                },
                "login": {
                  "nameClaimType": "http://schemas.xmlsoap.org/ws/2005/05/identity/claims/name",
                  "scopes": [],
                  "loginParameterNames": []
                }
              }
            }
          }
        }
    }