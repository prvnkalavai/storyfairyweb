{
    "routes": [
        {
            "route": "/api/*",
            "headers": {
                "X-Ms-Skip-Routing-Config": "true"
            }
        }
    ],
    "platform": {
        "apiRuntime": "python:3.10"
    },
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": ["/.auth/*", "/api/*"]
    }
}