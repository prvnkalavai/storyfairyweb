{
    "routes": [],
    "platform": {
        "apiRuntime": "python:3.10"
    },
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": ["/.auth/*", "/api/*"]
    },
    "auth": {
        "identityProviders": {
            "azureActiveDirectory": {
                "registration": {
                    "openIdIssuer": "https://<Your_Tenant_Domain>.b2clogin.com/<Your_Tenant_ID>/v2.0/",
                    "clientSecretSettingName": "AZURE_CLIENT_SECRET"
                },
                "userDetailsClaim": "http://schemas.xmlsoap.org/ws/2005/05/identity/claims/name"
            }
        }
    }
  }