{
    "routes": [
        {
            "route": "/api/*",
            "methods": ["GET", "POST"],
            "allowedRoles": ["authenticated"]
        }
    ],
    "platform": {
        "apiRuntime": "python:3.10"
    },
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": ["/.auth/*"]
    }
  }